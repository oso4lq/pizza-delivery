{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "angular",
  "installCommand": "pnpm install",
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist/pizza-delivery/browser",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}